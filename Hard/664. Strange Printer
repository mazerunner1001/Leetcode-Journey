// using dp solution with states of dp, base case and recurrence relation as follows
// dp[i][j] = minimum turns to print substring s[i..j]
// dp[i][i] = 1  (a single character always takes 1 turn)
// dp[i][j] = dp[i][j-1] + 1   // naive: print s[i..j-1] first, then s[j] separately
// But! If there exists k in [i, j-1] where s[k] == s[j],
// we can merge the printing of s[k] and s[j]:
// dp[i][j] = min(dp[i][j], dp[i][k] + dp[k+1][j-1])

class Solution {
public:
    int strangePrinter(string s) {
        int n = s.size();
        if (n == 0) return 0;

        string t = "";
        for (char c : s)
            if (t.empty() || t.back() != c)
                t.push_back(c);

        n = t.size();
        vector<vector<int>> dp(n, vector<int>(n, 0));

        for (int i = 0; i < n; i++) dp[i][i] = 1;

        for (int len = 2; len <= n; len++) {
            for (int i = 0; i + len - 1 < n; i++) {
                int j = i + len - 1;
                dp[i][j] = dp[i][j - 1] + 1;
                for (int k = i; k < j; k++) {
                    if (t[k] == t[j])
                        dp[i][j] = min(dp[i][j], dp[i][k] + dp[k + 1][j - 1]);
                }
            }
        }

        return dp[0][n - 1];
    }
};
